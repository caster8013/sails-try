<form action="/session/create" method="POST" class="form-signin">
  <% if(flash && flash.err) { %> 
    <ul class = "alert alert-success">
    <% Object.keys(flash.err).forEach(function(key) { %> 
    <li>
    <%= JSON.stringify(flash.err[key]) %> 
    </li>
    <% }) %> 
    </ul>
  <% } %>

  <div class="form-group">
    <label for="email">Email</label>
    <input type="email" class="form-control stored" id="email" name="email" placeholder="Email" required>
  </div>
  <div class="form-group">
    <label for="pwd">Password</label>
    <input type="password" class="form-control stored" data-parsley-length="[6, 32]" id="password" name="password"
      placeholder="Password">
  </div>

  <button type="submit" class="btn btn-primary" id="submit">Submit</button>
  <input type="hidden" name="_csrf" value="<%= _csrf %>" />
</form>
